{
  "name": "svelte-app",
  "main": "build/index.js",
  "version": "1.0.0",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.2.3",
    "svelte": "^2.16.1",
    "svelte-jest": "^0.2.0"
  },
  "peerDependencies": {
    "svelte": "*"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=build rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest",
      "\\.html$": "svelte-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "html"
    ]
  },
  "licence": "MIT"
}
